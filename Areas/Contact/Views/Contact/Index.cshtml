@model IEnumerable<ContactModel>
@{
    Layout = "_LayoutAdmin";
}
<div class="row page-titles mx-0">
    <div class="col p-md-0">
        <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="javascript:void(0)">Dashboard</a></li>
            <li class="breadcrumb-item active"><a href="javascript:void(0)">@Url.Action("contact")</a></li>
        </ol>
    </div>
</div>
<!-- row -->

<div class="container-fluid">
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title" onclick="test()">Danh sách liên hệ</h4>
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered zero-configuration">
                            <thead>
                                <tr>
                                    <th>STT</th>
                                    <th> @Html.DisplayNameFor(model => model.FullName)</th>
                                    <th> @Html.DisplayNameFor(model => model.Email)</th>
                                    <th> @Html.DisplayNameFor(model => model.DateSend)</th>
                                    <th> @Html.DisplayNameFor(model => model.Status)</th>
                                    <th> Chức năng</th>
                                </tr>
                            </thead>
                            <tbody class="listContact">
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="confirm"></div>
<div class="details"></div>
@* <div class="loading"></div> *@

<body>
    
    @* <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script> *@
        <script type="module" src='~/js/Contact.js'></script>

</body>